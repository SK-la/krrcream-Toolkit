<Window x:Class="krrTools.Tools.Converter.PresetWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="预设 Preset" Height="500" Width="300"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize">
    <Grid Margin="20">
        <StackPanel>
            <Button x:Name="DefaultPresetButton" Content="默认 Default" Margin="0,0,0,10" 
                    Click="DefaultPresetButton_Click" Height="30"/>
            <Button x:Name="TenKeyPresetButton" Content="10K" Margin="0,0,0,10" 
                    Click="TenKeyPresetButton_Click" Height="30"/>
            <Button x:Name="EightKeyPresetButton" Content="8K" Margin="0,0,0,10" 
                    Click="EightKeyPresetButton_Click" Height="30"/>
            <Button x:Name="SevenKeyPresetButton" Content="7K" 
                    Click="SevenKeyPresetButton_Click" Height="30"/>
        </StackPanel>
    </Grid>
</Window>